{
  "name": "tree-sitter-mocha",
  "version": "1.0.0",
  "description": "Mocha language highlighter",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "lexer"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tree-sitter generate && node-gyp build",
    "install": "node-gyp rebuild"
  },
  "author": "mini-rose",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.7"
  },
  "tree-sitter": {
    "scope": "source.ff",
    "file-types": [
      "ff"
    ]
  },
  "gypfile": true,
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mini-rose/tree-sitter-mocha.git"
  },
  "bugs": {
    "url": "https://github.com/mini-rose/tree-sitter-mocha/issues"
  },
  "homepage": "https://github.com/mini-rose/tree-sitter-mocha#readme"
}
